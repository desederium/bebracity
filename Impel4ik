		local function waitforwin()
			task.spawn(function() -- no afk winzone
				if getgenv().autodrop == true then
					local filecheck = pcall(function()

						Data = game:GetService('HttpService'):JSONDecode(readfile("tradefunction_"..game.Players.LocalPlayer.Name..".txt"))

					end)

					local trade

					if not filecheck then
						Data = {
							["trade"] = false,
						}
						writefile("tradefunction_"..game.Players.LocalPlayer.Name..".txt", game:GetService('HttpService'):JSONEncode(Data))
					else
						trade = Data["trade"]
					end

					if trade then
						game:GetService("TeleportService"):Teleport(1730877806, game:GetService("Players").LocalPlayer)
					end
				end

				repeat task.wait() until game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("BattleReportGui")

				task.spawn(function()
					repeat task.wait() until game.Workspace.NPCs:FindFirstChild("Shop Keeper")

					game.Workspace.NPCs:FindFirstChild("Shop Keeper"):Destroy()
				end)

				local playerTime = os.date("*t", os.time())
				local hour, min, sec = playerTime.hour, playerTime.min, playerTime.sec
				if playerTime.hour >= 24 then
					hour = (tonumber(hour) - 24)
				end
				if playerTime.min < 10 then
					min = ("0"..playerTime.min)
				end
				pcall(function()
					local response = request({
						Url = getgenv().iwantyourWebhook,
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"
						},
						Body = game.HttpService:JSONEncode({
							content = "",
							embeds = {
								{
									title = titlehero,
									description = "IMPEL CLEAR!",
									color = 65280,
									fields = {
										{
											name = "Player:",
											value = ("||"..game.Players.LocalPlayer.Name.."|| <- click me")
										},
										{
											name = "took:",
											value = (tostring(game:GetService("Players").LocalPlayer.PlayerGui.BattleReportGui:WaitForChild("BattleReport"):WaitForChild("LeftContainer"):WaitForChild("Clear Time"):WaitForChild("Display").Text))
										},
										{
											name = "points:",
											value = (tostring(game:GetService("Players").LocalPlayer.PlayerGui.BattleReportGui:FindFirstChild("Points").Text))
										},
										{
											name = "sent on:",
											value = (tostring((hour + getgenv().timediff) .. ":" .. min .. ":" .. sec))
										}
									}
								}
							},
							username = "Neo",
							avatar_url = ava,
							attachments = {}
						})
					})
				end)

				local points = tonumber(game:GetService("Players").LocalPlayer.PlayerGui.BattleReportGui:FindFirstChild("Points").Text)

				local legfarm = false
				local mythfarm = false

				repeat task.wait()
				until game.Workspace.Islands["Impel Base - Finished"].Environment:FindFirstChild("Requeue")

				local area = game.Workspace.Islands["Impel Base - Finished"].Environment:FindFirstChild("Requeue")
				local ti = TweenInfo.new(5, Enum.EasingStyle.Linear)
				local desired = area.CFrame
				local tweentp = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), ti, {CFrame = desired})

				local function farmleg()

					if game:GetService("Workspace").Islands["Impel Base - Finished"].Models:FindFirstChild("Legendary Fruit Chest") then

						local legchest = game:GetService("Workspace").Islands["Impel Base - Finished"].Models:FindFirstChild("Legendary Fruit Chest")

						local proxp = nil

						if legchest.Part:FindFirstChild("ProximityPrompt") then
							proxp = legchest.Part:FindFirstChild("ProximityPrompt")
						end

						local legnums = string.gmatch(proxp.ActionText, "%d+")
						local points2 = tonumber(game:GetService("Players").LocalPlayer.PlayerGui.BattleReportGui:FindFirstChild("Points").Text)
						local neededpoints2

						for digit in legnums do 

							if tonumber(digit) > 255 then

								neededpoints2 = tonumber(digit)

							end

						end

						if points2 >= neededpoints2 then

							local desired3 = legchest.PrimaryPart.CFrame

							local tweentp3 = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), ti, {CFrame = desired3})

							tweentp3:Play()

							task.wait(ti.Time)

							task.wait(1)

							if not proxp then
								proxp = legchest.Part:FindFirstChild("ProximityPrompt")
							end

							local x = math.floor(points2/neededpoints2)

							if x >= 5 then

								x = 5

							end

							if x > 1 then
								for i = 1, x, 1 do

									fireproximityprompt(proxp) print("firing...") task.wait(3) 

								end

								local y = (points2-tonumber(game:GetService("Players").LocalPlayer.PlayerGui.BattleReportGui:FindFirstChild("Points").Text))

								local z = y/neededpoints2

								if z >= 1 then
									if getgenv().pingonchests == true then
										pcall(function()
											local response = request({
												Url = getgenv().iwantyourWebhook,
												Method = "POST",
												Headers = {
													["Content-Type"] = "application/json"
												},
