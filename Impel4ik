				local function farmmyth()
					if game.Workspace.Islands["Impel Base - Finished"].Models:FindFirstChild("Mythical Fruit Chest") then

						local chest = game.Workspace.Islands["Impel Base - Finished"].Models:FindFirstChild("Mythical Fruit Chest")
						local ti = TweenInfo.new(5, Enum.EasingStyle.Linear)

						local desired = chest.PrimaryPart.CFrame

						local tweentp5 = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"), ti, {CFrame = desired})

						--POINT CHECK

						local pp = nil

						if chest.Part:FindFirstChild("ProximityPrompt") then
							pp = chest.Part:FindFirstChild("ProximityPrompt")

							local nums = string.gmatch(pp.ActionText, "%d+")
							local neededpoints

							for digit in nums do 

								if tonumber(digit) > 255 then

									neededpoints = tonumber(digit)

								end

							end


							if points >= neededpoints  then
								if getgenv().bomjcap == 0 or getgenv().bomjcap >= neededpoints then
									local x = math.floor(points/neededpoints)

									if x >= 3 then

										x = 3

									end

									tweentp5:Play()
									task.wait(ti.Time)

									print("ended")

									for i = 1, x, 1 do
										fireproximityprompt(pp) task.wait(3)
									end


									if points ~= tonumber(game:GetService("Players").LocalPlayer.PlayerGui.BattleReportGui:FindFirstChild("Points").Text) then
										local ihave = (points-tonumber(game:GetService("Players").LocalPlayer.PlayerGui.BattleReportGui:FindFirstChild("Points").Text))
										local ibought = (ihave/neededpoints)
										if getgenv().pingonchests == true then
											local succ, err = pcall(function()
												local response = request({
													Url = getgenv().iwantyourWebhook,
													Method = "POST",
													Headers = {
														["Content-Type"] = "application/json"
													},
