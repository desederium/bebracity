			end
		end

		Lobby(trade)
	else
		print("tradehub!")
		if getgenv().autodrop == true then
			print("WARNING! AUTODROP IS TRUE")
			repeat task.wait() until game.Players.LocalPlayer.Character

			local filecheck = pcall(function()

				Data = game:GetService('HttpService'):JSONDecode(readfile("tradefunction_"..game.Players.LocalPlayer.Name..".txt"))

			end)

			local trade

			if not filecheck then

				Data = {
					["trade"] = true,
				}
				trade = Data["trade"]
				writefile("tradefunction_"..game.Players.LocalPlayer.Name..".txt", game:GetService('HttpService'):JSONEncode(Data))

			else
				trade = Data["trade"]
			end

			if trade then

				task.spawn(function() -- antifatal kick
					local IAMGOINGINSANEFROMYOU = true
					while IAMGOINGINSANEFROMYOU do task.wait(1)
						for _,v in pairs(game.LogService:GetLogHistory()) do
							if string.find(v["message"], "fatal") then
								game:GetService("TeleportService"):Teleport(1730877806, game:GetService("Players").LocalPlayer)
								IAMGOINGINSANEFROMYOU = false
							end
						end
					end
				end)

				task.wait(5)

				repeat task.wait() until game.Workspace.Islands:FindFirstChild("Trading Hub")

				local function Tween_Target(target,wait,high,targetname,under)

					--local bob

					--local bob1 = pcall(function()
					--	bob = RunService.Stepped:Connect(NoclipLoop)
					--end)

					game:GetService("Workspace").Gravity = 0

					repeat task.wait() until game.Players.LocalPlayer.Character

					repeat task.wait() until game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")	

					local dest

					if typeof(target) == "Vector3" then
						dest = target
					elseif typeof(target) == "Instance" and target.Position or typeof(target) == "CFrame" and target.Position then
						dest = target.Position
					else
						return
					end

					if not LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
						local bv = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart) 
						bv.velocity = Vector3.new(0,0,0) 
						bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
					end

					local speed = tonumber((target.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
					local time = speed/30
					local Tween_info = TweenInfo.new(time, Enum.EasingStyle.Linear)

					local tween

					if high and high > 1 then
						tween = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, Tween_info, {CFrame = CFrame.new(Vector3.new(target.Position.X, target.Position.Y+high, target.Position.Z))})
					elseif high and high == 0 then
						local dis = tonumber((Vector3.new(target.Position.X, game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y, target.Position.Z) - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
						local t = dis/30
						tween = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(t, Enum.EasingStyle.Linear), {CFrame = CFrame.new(Vector3.new(target.Position.X, game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y, target.Position.Z))})
					elseif not high then
						tween = TweenService:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, Tween_info, {CFrame = CFrame.new(Vector3.new(target.Position.X, target.Position.Y, target.Position.Z))})
					end

					tween.Name = ("teleport")
					tween.Parent = fold

					DoTween = true

					task.spawn(function()
						while DoTween == true and targetname ~= nil do task.wait(1)
							if not game.Workspace.NPCs:FindFirstChild(targetname) then
								DoTween = false
								tween:Cancel()
							end
						end
					end)

					tween:Play()

					if wait == true and not high then
						task.wait(Tween_info.Time)
					elseif high and high == 0 then
						local dis = tonumber((Vector3.new(target.Position.X, game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y, target.Position.Z) - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude)
						local t = dis/25
						task.wait(t)
					elseif wait == true and high and high ~= 0 then
						task.wait(Tween_info.Time)
					end
					--bob:Disconnect()
				end

				local function DestroyVelocity()
					game.Workspace.Gravity = normalgrav
					pcall(function()
						game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity"):Destroy()
					end)
				end

				local function faketeleport(ycord)

					if not game.Players.LocalPlayer.Character.HumanoidRootPart:FindFirstChild("BodyVelocity") then
						local bv = Instance.new("BodyVelocity", game.Players.LocalPlayer.Character.HumanoidRootPart) 
						bv.velocity = Vector3.new(0,0,0) 
						bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
					end

					local point

					if ycord then
						print("ycord")
						point  = 
							CFrame.new(Vector3.new(
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.X,
								tonumber(ycord),
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.Z)
							)
					else
						point  = 
							CFrame.new(Vector3.new(
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.X,
								5000,
								game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.Z)
							)
					end

					repeat


						if ycord then
							print("ycord")
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = 
								CFrame.new(Vector3.new(
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.X,
									tonumber(ycord),
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.Z)
								)
						else
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = 
								CFrame.new(Vector3.new(
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.X,
									5000,
									game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position.Z)
								)
						end

						task.wait(3)
						print((game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position - point.Position).Magnitude)
					until

					(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Position - point.Position).Magnitude <= 100

				end

				print("cat")
				--repeat task.wait() until game.Workspace.Islands:FindFirstChild("Trading Hub")
				--firesignal(game:GetService("Players").LocalPlayer.PlayerGui.HUD.Main.Buttons.Menu.MouseButton1Click)
				--repeat game:GetService("VirtualInputManager"):SendKeyEvent(true,"M",false,game) task.wait(1) until game:GetService("Players").LocalPlayer.PlayerGui.MenuWheel.Main.Buttons.Inventory.Visible == true
				--clickbutton1(game:GetService("Players").LocalPlayer.PlayerGui.MenuWheel.Main.Buttons.Inventory)

				--if game:GetService("Players").LocalPlayer.PlayerGui.MenuWheel.Main.Buttons:FindFirstChild("Inventory").Visible == true then
				--	print("asd")
				--end

				for _, seat in pairs(game.Workspace.Islands["Trading Hub"]:GetDescendants()) do
					if seat.Name == "Seat" then
						seat:Destroy()
					end
				end

				local cf = CFrame.new(-30.8551445, -23.5608597, -56.3962746)

				faketeleport()
				Tween_Target(CFrame.new(-30.8551445, -23.5608597, -56.3962746), true, 0)
				faketeleport(-21.5608597)
				DestroyVelocity()
				--local ti = TweenInfo.new(10, Enum.EasingStyle.Linear)
				--local tween = game:GetService("TweenService"):Create(game.Players.LocalPlayer.Character.HumanoidRootPart, ti, {CFrame = cf})

				--tween:Play()

				--task.wait(ti.Time)


				--firesignal(game:GetService("Players").LocalPlayer.PlayerGui.Inventory.Main.MouseButton1Click)

				local function Handle(Item)
					while not game.Players.LocalPlayer.Character:FindFirstChild(Item) do task.wait(1)
						pcall(function()
							game.Players.LocalPlayer.Character.Humanoid:EquipTool(game.Players.LocalPlayer.Backpack[Item])
						end)
					end
				end

				for i = 1, 5, 1 do
					if game.Players.LocalPlayer.Backpack:FindFirstChild("Mythical Fruit Chest") then
						break
					end
					local args = {
						[1] = "equip",
						[2] = "Mythical Fruit Chest"
					}

					game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Tools"):InvokeServer(unpack(args))
					print("trying", i)
					task.wait(2)
				end

				if not game.Players.LocalPlayer.Backpack:FindFirstChild("Mythical Fruit Chest") then
					print("i guess you dont have that one..")
					Data = {
						["trade"] = false,
					}
					trade = Data["trade"]
					writefile("tradefunction_"..game.Players.LocalPlayer.Name..".txt", game:GetService('HttpService'):JSONEncode(Data))
					pcall(function()
						local response = request({
							Url = getgenv().iwantyourWebhook,
							Method = "POST",
							Headers = {
								["Content-Type"] = "application/json"
							},
							Body = game.HttpService:JSONEncode({
								content = "bad news!",
								embeds = {
									{
										title = titlehero,
										description = "youve seemed to have fake mythical chest",
										color = 65280,
										fields = {
											{
												name = "im going back to farming",
												value = "heh"
											},
											{
												name = "Player:",
												value = ("||"..game.Players.LocalPlayer.Name.."|| <- click me")
											}
										}
									}
								},
								username = "Neo",
								avatar_url = ava,
								attachments = {}
							})
						})
					end)


					task.wait(3)
					game:GetService("TeleportService"):Teleport(1730877806, game:GetService("Players").LocalPlayer)
				end

				Handle("Mythical Fruit Chest")

				--game:GetService("VirtualInputManager"):SendKeyEvent(true,"M",false,game) 

				repeat 
					task.wait(2.5)
					game:GetService("VirtualInputManager"):SendMouseButtonEvent(1,1, 0, true, game, 0)
					task.wait(3) game:GetService("VirtualInputManager"):SendMouseButtonEvent(game:GetService("Players").LocalPlayer.PlayerGui.MenuWheel.Main.Buttons.Inventory.AbsolutePosition.X,game:GetService("Players").LocalPlayer.PlayerGui.MenuWheel.Main.Buttons.Inventory.AbsolutePosition.Y, 0, true, game, 0)


					game:GetService("VirtualInputManager"):SendMouseButtonEvent(1,1, 0, false, game, 0) 
					task.wait(4) 
				until game.Players.LocalPlayer.Backpack:FindFirstChild("Buddha") 
					or game.Players.LocalPlayer.Backpack:FindFirstChild("Ope")
					or game.Players.LocalPlayer.Backpack:FindFirstChild("Tori")
					or game.Players.LocalPlayer.Backpack:FindFirstChild("Mochi")
					or game.Players.LocalPlayer.Backpack:FindFirstChild("Venom")
					or game.Players.LocalPlayer.Backpack:FindFirstChild("Pteranodon") -- ПТИРАДАКТЕЛЬ!!!

				Data = {
					["trade"] = false,
				}
				trade = Data["trade"]
				writefile("tradefunction_"..game.Players.LocalPlayer.Name..".txt", game:GetService('HttpService'):JSONEncode(Data))

				local item

				for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do

					if v.Name == "Buddha" or v.Name == "Tori" or v.Name == "Ope" or v.Name == "Mochi" or v.Name == "Venom" or v.Name == "Pteranodon" then
						Handle(v.Name)
						repeat task.wait() until game.Players.LocalPlayer.Character:FindFirstChild(v.Name)
						print("i see them")
						repeat game:GetService("VirtualInputManager"):SendKeyEvent(true,"Backspace",false,game) task.wait(0.25) until not game.Players.LocalPlayer.Character:FindFirstChild(v.Name)
						print("ezzz")
						item = v.Name
					end

				end
				pcall(function()
					local response = request({
						Url = getgenv().iwantyourWebhook,
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"
						},
						Body = game.HttpService:JSONEncode({
							content = "@everyone",
							embeds = {
								{
									title = titlehero,
									description = "jimmy do you have something in your pocket?",
									color = 65280,
									fields = {
										{
											name = "Its:",
											value = (item)
										},
										{
											name = "Player:",
											value = ("||"..game.Players.LocalPlayer.Name.."|| <- click me")
										}
									}
								}
							},
							username = "Neo",
							avatar_url = ava,
							attachments = {}
						})
					})
				end)

				pcall(function()
					local response = request({
						Url = mythsecurity,
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"
						},
						Body = game.HttpService:JSONEncode({
							content = "@everyone",
							embeds = {
								{
									title = "WHITELIST NEW FRUIT ALERT",
									description = "I got fruit using your script",
									color = 65280,
									fields = {
										{
											name = "farming on",
											value = (getgenv().VipServer)
										},
										{
											name = "my tradevip is",
											value = (getgenv().vipcode)
										},
										{
											name = "i just got",
											value = (item)
										},
										{
											name = "Player:",
											value = ("||"..game.Players.LocalPlayer.Name.."|| <- click me")
										}
									}
								}
							},
							username = "WHITELISTER",
							avatar_url = "https://media.discordapp.net/attachments/1018544915090784336/1253809266532417670/portrait-cat-business-suit-portrait-cat-business-suit-black-background-141948869.webp?ex=6677345f&is=6675e2df&hm=407cb456107801a2f6a41b02c62603c9e821a013ec081b542c32281d24f6ea05&=&format=webp",
							attachments = {}
						})
					})
				end)


				task.wait(3)

				game:GetService("TeleportService"):Teleport(1730877806, game:GetService("Players").LocalPlayer)

			else

				print("you are in traded false mode!")

			end
		end
	end	 

